{
  "name": "hamlbars-highlighter",
  "displayName": "Hamlbars Highlighter",
  "description": "Hamlbars syntex highlighter and snippets",
  "version": "0.0.1",
  "publisher": "Iurii Bairamukov",
  "engines": {
    "vscode": "^1.79.0"
  },
  "categories": [
		"Programming Languages",
		"Snippets"
	],
  "activationEvents": [
    "onLanguage:hamlbars"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/Comediant24/hamlbars-highlighter.git"
  },
  "main": "./extension.js",
  "contributes": {
    "languages": [
			{
				"id": "hamlbars",
				"aliases": [
					"hamlbars",
					"hamlbars"
				],
				"extensions": [
					".hamlbars"
				],
				"configuration": "./hamlbars.configuration.json"
			}
		],
		"grammars": [
			{
				"language": "hamlbars",
				"scopeName": "text.hamlbars",
				"path": "./syntaxes/hamlbars.json"
			}
		],
		"snippets": [
			{
				"language": "hamlbars",
				"path": "./snippets/snippets.json"
			}
		]
  },
  "scripts": {
    "lint": "eslint .",
    "pretest": "npm run lint",
    "test": "node ./test/runTest.js"
  },
  "devDependencies": {
    "@types/vscode": "^1.79.0",
    "@types/glob": "^8.1.0",
    "@types/mocha": "^10.0.1",
    "@types/node": "20.2.5",
    "eslint": "^8.41.0",
    "glob": "^8.1.0",
    "mocha": "^10.2.0",
    "typescript": "^5.1.3",
    "@vscode/test-electron": "^2.3.2"
  }
}
